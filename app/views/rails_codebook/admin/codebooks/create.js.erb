$('#add-codebook').modal('hide');
binding.pry
partial = '<%= escape_javascript(render 'codebook', value: @digest) %>';
codebook = "#codebook_<%= @digest %>";

if( $(codebook).size() > 0 ){

  $(codebook).replaceWith(partial);

} else {

  $('#codebooks-table tbody').prepend(' <%= escape_javascript(render 'codebook', value: @digest) %> ');

}
