$('#add-codebook').modal('hide');
partial = '<%= escape_javascript(render 'codebook', id: @codebook.id) %>';
codebook = "#codebook_<%= @codebook.id %>";

if( $(codebook).size() > 0 ){
  $(codebook).replaceWith(partial);
} else {
  $('#codebooks-table tbody').prepend(' <%= escape_javascript(render 'codebook', id: @codebook.id ) %> ');
}
